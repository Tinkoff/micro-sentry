    "@nrwl/eslint-plugin-nx": "11.6.3",
    "@nrwl/jest": "11.6.3",
    "@nrwl/nx-cloud": "11.2.0",
    "@nrwl/tao": "11.6.3",
    "@nrwl/workspace": "11.6.3",
    "@types/express": "^4.17.0",
    "@types/jest": "26.0.8",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
