    "@nrwl/nx-cloud": "14.0.8",
    "@nrwl/workspace": "14.2.4",
    "@types/express": "^4.17.0",
    "@types/jest": "27.4.1",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
